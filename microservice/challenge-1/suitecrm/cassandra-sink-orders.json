{
  "name": "cassandra-sink-orders",
  "config": {
    "connector.class": "io.confluent.connect.cassandra.CassandraSinkConnector",
    "cassandra.contact.points": "cassandra",
    "tasks.max": "1",
    "topics": "orders-stream",
    "transforms": "dropPrefix",
    "transforms.dropPrefix.type": "org.apache.kafka.connect.transforms.RegexRouter",
    "transforms.dropPrefix.regex": "(.*)",
    "transforms.dropPrefix.replacement": "orders",
    "cassandra.username": "root",
    "cassandra.keyspace.create.enabled": "true",
    "cassandra.port": "9042",
    "cassandra.keyspace": "suitecrm",
    "cassandra.table.manage.enabled": "true",
    "value.converter.schema.registry.url": "http://localhost:8081",
    "cassandra.write.mode": "Update",
    "cassandra.password": "root",
    "cassandra.consistency.level": "LOCAL_ONE",
    "value.converter": "io.confluent.connect.avro.AvroConverter",
    "key.converter": "io.confluent.connect.avro.AvroConverter",
    "key.converter.schema.registry.url": "http://localhost:8081"
  }
}